RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?ds-music-france.com$
RewriteCond %{REQUEST_URI} !^/public/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /public/$1
RewriteCond %{HTTP_HOST} ^(www.)?ds-music-france.com$
RewriteRule ^(/)?$ public/index.php [L]